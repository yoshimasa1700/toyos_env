   (quote
    ("49e5a7955b853f70d1fe751b2f896921398b273aa62f47bda961a45f80219581" default)))
 '(package-selected-packages
   (quote
    (flycheck-haskell smart-jump dumb-jump avy-migemo counsel company-ghci company-ghc haskell-mode powerline company-irony irony exec-path-from-shell tangotango-theme color-theme helm flymake-python-pyflakes py-autopep8 jedi flymake-cursor flymake-jslint flycheck pos-tip fuzzy auto-complete))))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
